{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "Posts"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "data": {
      "type": "richtext"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "published_at": {
      "type": "datetime"
    },
    "media": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "model": "categories"
    }
  }
}
